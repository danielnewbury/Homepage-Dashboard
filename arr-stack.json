- Arr:
    - Sonarr:
        href: https://sonarr.newbury.inthearea.party
        icon: sonarr
        description: Grafana â†’ Arr Stack dashboard
        widgets:
          - prometheus:
              label: CPU
              query: rate(container_cpu_usage_seconds_total{container="sonarr"}[1m])
              unit: cores
          - prometheus:
              label: Memory
              query: container_memory_usage_bytes{container="sonarr"}
              unit: bytes
